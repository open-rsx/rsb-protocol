IF(${PACKAGE_FIND_VERSION_PATCH} EQUAL @RSB_PROTOCOL_WIRE_FORMAT_VERSION@)
  MESSAGE(STATUS "RSBProtocol: Matching wire-format version (@RSB_PROTOCOL_WIRE_FORMAT_VERSION@)")
  SET(PACKAGE_VERSION "@RSB_PROTOCOL_VERSION_MAJOR@.@RSB_PROTOCOL_VERSION_MINOR@.@RSB_PROTOCOL_VERSION_PATCH@")
  SET(PACKAGE_VERSION_COMPATIBLE TRUE)
ELSE()
  MESSAGE("RSBProtocol: Wire-format version mismatch; Requested: ${PACKAGE_FIND_VERSION_PATCH}; Provided: @RSB_PROTOCOL_WIRE_FORMAT_VERSION@")
  SET(PACKAGE_VERSION_COMPATIBLE FALSE)
ENDIF()
